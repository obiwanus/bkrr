        else:
            renpy.show("bkrr_timeskip_logo")
        renpy.with_statement(bkrr_timeskip_transition(2.0))
        renpy.hide("bkrr_timeskip_logo")
        renpy.with_statement(bkrr_timeskip_transition(2.0))
        if sounded:
            renpy.music.stop(channel="sound", fadeout=2.0)

