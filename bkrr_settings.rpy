    # Функция, возвращающая текущее время суток

    def bkrr_get_usertime():
        from time import strftime, localtime
        time = strftime("%H:%M:%S", localtime())
        hour, min, sec = time.split(":")
        hour = int(hour)
        if 9 < hour < 19:
            return "day"
        elif 5 < hour > 18:
            return "sunset"
        elif 20 > hour >= 0:
            return "night"

